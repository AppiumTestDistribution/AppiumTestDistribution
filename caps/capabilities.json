    "shouldUseSingletonTestManager": false,
    "simpleIsVisibleCheck": true,
    "wdaStartupRetries": 10,
    "wdaStartupRetryInterval": 100,
    "maxTypingFrequency": 10
  },
  "hostMachines": [
    {
      "machineIP": "127.0.0.1",
      "simulators": [
        {
          "deviceName": "iPhone 11",
          "OS": "14.0"
        },
        {
          "deviceName": "iPhone 8",
          "OS": "14.0"
        }
      ]
    }
  ]
