{
  "android": {
    "platformName": "android",
    "platformVersion": "7.0",
    "automationName": "UiAutomator2",
    "uiautomator2ServerInstallTimeout": 50000,
    "adbExecTimeout": 60000,
    "app": {
      "local": "./apps/VodQA.apk"
    },
    "deviceName": "android",
    "noSign": true
  },
  "iOS": {
    "platformName": "iOS",
    "app": {
      "simulator": "./apps/VodQAReactNative.zip"
    },
    "deviceName": "iPhone",
    "wdaLaunchTimeout": 80000,
    "wdaConnectionTimeout": 80000,
    "preventWDAAttachments": true,
